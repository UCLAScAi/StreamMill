TABLE A(a Int, b Int);
TABLE B(a Int, b Int);
INSERT INTO A VALUES(1, 2);
INSERT INTO A VALUES(3, 4);
INSERT INTO B VALUES(5, 6);
SELECT * FROM A;
SELECT * FROM A, B;
SELECT A.*, B.b FROM A, B;
SELECT * FROM (SELECT a, b FROM A) AS C;
SELECT * FROM (SELECT * FROM A) AS C;
SELECT * FROM (SELECT * FROM A) AS C, B;
SELECT * FROM (SELECT sum(a) AS s1, sum(b) + count(a) AS s2 FROM A) AS B;
#SELECT * FROM (SELECT sum(a) FROM A) AS C;
